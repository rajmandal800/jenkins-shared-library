{
  "extends": "recommended",
  "rules": {
    "CompileStatic": "off",
    "NoDef": "off",
    "VariableTypeRequired": "off",
    "MethodReturnTypeRequired": "off",
    "UnnecessaryGString": "off",
    "UnnecessaryPublicModifier": "off",
    "SpaceAroundMapEntryColon": {
      "characterBeforeColonRegex": "\\S",
      "characterAfterColonRegex": "\\s"
    }
  },
  "env": {
    "jenkins": true
  }
}
